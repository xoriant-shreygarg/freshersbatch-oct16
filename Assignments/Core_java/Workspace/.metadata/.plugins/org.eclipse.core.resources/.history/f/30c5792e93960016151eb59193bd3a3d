package com.nestedclasses;

import java.util.Date;
import java.util.HashMap;

class MyApp{

	HashMap<String, Cache> data = new HashMap<>();
	
	class Cache{
		
		Date date;
		String key;
		String value;
		
		public Cache(String key, String value) {
			super();
			this.date = new Date();
			this.key = key;
			this.value = value;
		}	
		
	}
	
	public void add(String key, String value){
		data.put(key, new Cache(key, value));
	}
	
	public Cache get(String key){
		return data.get(key);
	} 
	
}

public class AppMain {

	public static void main(String[] args) {
		MyApp app = new MyApp();
		app.add(key, value);
	}

}
