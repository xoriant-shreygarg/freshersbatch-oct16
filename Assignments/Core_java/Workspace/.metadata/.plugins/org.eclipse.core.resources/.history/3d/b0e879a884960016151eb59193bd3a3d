package com.annotations;

import java.lang.reflect.Method;

class Test{
	private String name;
	private int age;
	
	@Getter
	public String getName() {
		return name;
	}
	
	@Getter
	public int getAge() {
		return age;
	}
}

public class Annotation2 {

	public static void main(String[] args) {
		Method m[] = Test.class.getDeclaredMethods();
		
	}

}
