package com.collections;

import java.util.*;

class Employee{
	private int number;
	public Employee(int number) {
		super();
		this.number = number;
	}
	public int getNumber() {
		return number;
	}
	public void setNumber(int number) {
		this.number = number;
	}
	@Override
	public int hashCode() {
		return 1;
	}
	@Override
	public boolean equals(Object obj) {
		return true;
	}
}

public class Collections5 {

	public static void main(String[] args) {
		Map<Employee, String> hashMap = new HashMap<>();
		hashMap.put(new Employee(1), "A");
		hashMap.put(new Employee(2), "B");
		hashMap.put(new Employee(3), "C");
		hashMap.put(new Employee(4), "D");

		System.out.println(hashMap.get(d1));
	}

}