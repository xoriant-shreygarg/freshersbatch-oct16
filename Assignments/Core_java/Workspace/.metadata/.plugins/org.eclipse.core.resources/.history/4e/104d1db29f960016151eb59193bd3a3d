package com.nestedclasses;

import java.util.Date;
import java.util.HashMap;

class DataWrapper{
	String value;
	Date date;
	
	public DataWrapper(String value, Date date) {
		super();
		this.value = value;
		this.date = date;
	}
	
}
class MyApp{
	
	Cache cache;
	
	public MyApp() {
		super();
		this.cache = this.new cache();
	}

	class Cache{
		HashMap<String, DataWrapper> data = new HashMap<>();
		public void add(String key, String value){
			data.put(key, new DataWrapper(value, new Date()));
		}
		public DataWrapper get(String key){
			return data.get(key);
		}
	}
	
}

public class AppMain {

	public static void main(String[] args) {
		MyApp app = new MyApp(.Cache());
	}

}
