class GenericWrapper<T>{
	T t;
	public T get() {
		return this.t;
	}
	public void set(T t) {
		this.t = t;
	}
}

public class Generics1 {
	
	public static void main(String[] args) {
		GenericWrapper genericWrapper = new GenericWrapper();
		String str = "ABC";
		genericWrapper.setObj(str);
		str = genericWrapper.getObj();
	}

}
