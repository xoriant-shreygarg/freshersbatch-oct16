package com.filehandling;

import java.io.File;
import java.io.FileInputStream;
import java.io.FileNotFoundException;

public class FileHandling4 {

	public static void main(String[] args) {
		readFiles();
	}

	private static void readFiles() throws FileNotFoundException {
		FileInputStream fis1 = new FileInputStream(new File("abc.txt"));
		FileInputStream fis2 = new FileInputStream(new File("pqr.txt"));
		FileInputStream fis3 = new FileInputStream(new File("account.txt"));
		
	}

}
