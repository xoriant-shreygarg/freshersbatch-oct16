package com.nestedclasses;

import java.util.Date;
import java.util.HashMap;

class MyApp{

	HashMap<String, Cache> data = new HashMap<>();
	
	class Cache{
		
		Date date;
		String key;
		String value;
		
		public Cache(Date date, String key, String value) {
			super();
			this.date = date;
			this.key = key;
			this.value = value;
		}	
		
	}
	
	public void add(Date date, String key, String value){
		data.put(key, new Cache(date, key, value));
	}
	
	public Cache get(String key){
		return data.get(key);
	} 
	
}

public class AppMain {

	public static void main(String[] args) {
		
	}

}
