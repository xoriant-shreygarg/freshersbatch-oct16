package com.mypackage;

import static org.junit.Assert.*;

import org.junit.Ignore;
import org.junit.Test;

public class StringOperationsGetLengthTest {

	@Test
	public void test1() {
		assertEquals(5, new StringOperations().getLength("hello"));
	}

	@Test
	public void test2() {
		assertEquals('e', new StringOperations().getCharacter("hello", 1));
	}
	
	@Test
	public void test3() {
		assertEquals('s', new StringOperations().getCharacter("shrey", 0));
	}
}
